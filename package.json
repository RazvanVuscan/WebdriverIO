{
  "name": "webdriverio-project",
  "version": "1.0.0",
  "description": "for playing around with",
  "main": "index.js",
  "scripts": {
    "config": "npx wdio config",
    "test": "npx wdio ./conf/wdio.conf.js PLATFORM=desktop",
    "android": "npx wdio ./conf/wdio.android.conf.js PLATFORM=mobile",
