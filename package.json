    "@wdio/cucumber-framework": "^6.1.22",
    "@wdio/local-runner": "^6.4.2",
    "allure-commandline": "^2.13.0",
    "appium": "^1.18.0",
    "chai": "^4.2.0",
    "chromedriver": "^83.0.1",
    "cucumber": "^6.0.5",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
